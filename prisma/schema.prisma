generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
model Email {
  id    Int     @id @default(autoincrement())
  user  Int
  draft Boolean
  created_at DateTime @default(now())
  last_edited DateTime @default(now())
}
